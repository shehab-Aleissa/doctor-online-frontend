{
  "expo": {
    "name": "Doctor Online App",
    "description": "This project is really great.",
    "slug": "doctor-app-frontend",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.doctoronline.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.doctoronline.app",
      "googleServicesFile": "./google-services.json"
    }
  }
}